{
  "name": "@kjots/express-json-data",
  "version": "3.2.1",
  "description": "Express JSON data router",
  "main": "lib/index.js",
  "types": "types.d.ts",
  "scripts": {
    "build": "babel src --out-dir lib --ignore .spec.js --source-maps --quiet",
    "build:watch": "babel src --out-dir lib --ignore .spec.js --source-maps --watch",
    "clean": "npm-run-all clean:coverage clean:lib",
    "clean:coverage": "rimraf .nyc_output coverage",
    "clean:lib": "rimraf lib",
    "prepare": "npm-run-all prepare:husky prepare:package",
    "prepare:husky": "husky install",
    "prepare:package": "npm-run-all clean build",
    "release": "standard-version",
    "test": "npm-run-all test:eslint test:mocha",
    "test:eslint": "eslint src",
    "test:mocha": "cross-env NODE_ENV=test nyc mocha --require babel-core/register --require babel-polyfill src/**/*.spec.js"
  },
  "homepage": "https://github.com/kjots/express-json-data#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjots/express-json-data.git"
  },
  "keywords": [
    "express",
    "json",
    "data",
    "router"
  ],
  "author": "Karl Juri Ots <kjots@kjots.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kjots/express-json-data/issues"
  },
  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "node-mocks-http": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "standard-version": "^9.3.2"
  },
  "peerDependencies": {
    "babel-polyfill": "^6.26.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fast-json-patch": "^3.1.0",
    "lodash": "^4.17.21"
  }
}
